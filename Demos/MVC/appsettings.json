{
  "MicrosoftLogging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "LiterateConsole": {
      "EventLevel": "Verbose",
      "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {MachineName} {Level}:{EventId} ({ThreadId}) [{SourceContext}] {Message}{NewLine}{Exception}"
    },
    "RollingFile": {
      "EventLevel": "Verbose",
      "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {MachineName} {Level}:{EventId} ({ThreadId}) [{SourceContext}] {Message}{NewLine}{Exception}",
      "Path": "Logs/aspvnext-{Date}.log",
      "FileSizeLimitMegaBytes": 1024,
      "RetainedFileCountLimit": 31
    }
  },
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Monkell-WebApp;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  },
  "Request": {
    "SessionIdleTimeoutInSeconds": 180,
    "MaxConcurrent": 200,
    "ClearGCAfterNumberOfRequest": 1000
  },
  "PSEngine": {
    "Protocol": "http",
    "ComputerName": "127.0.0.1",
    "Port": 5985,
    "ExecutionPolicy": "RemoteSigned",
    "WorkingDirectory": "/",
    "StartupScriptFile": "Scripts/test.ps1",
    "StartupScriptFileCacheEnabled": true,
    "MinRunspace": 50,
    "MaxRunspace": 100,
    "CleanupIntervalFromMinutes": 5,
    "ThreadOptions": "ReuseThread",
    "ApartmentState": "STA",
    "InitialStateMethod": "Create",
    "LanguageMode": "FullLanguage",
    "Variables": {
      "ErrorActionPreference": "Continue",
      "WarningPreference": "Continue",
      "VerbosePreference": "Continue",
      "InformationPreference": "Continue",
      "DebugPreference": "Continue",
      "ProgressPreference": "Continue",
      "PSModuleAutoloadingPreference": "none"
    },
    "Modules": {
    },
    "PSModulePath": [
      "Website\\Modules"
    ]
  }
}